package rp.core;


class Heap {
	data:void*;
    capacity:int8;
    size:int8;
    
    <T:any> clone(obj:T):T {
		let newObj = obj;
		//TODO
		return newObj;
	}

	<T:any> use(array:T*, start:int4, end:int4):T* {
		return array[start,end];
	}

	<T:any> alloc(size:int8, type:any):T* {
		return malloc(size, sizeof(type));
	}

	<T:any> free(obj:T*) {
		//TODO
		return free(obj);
	}

}


